{
  "name": "kickbot-sdk",
  "version": "0.1.1",
  "description": "A demo package for Total TypeScript",
  "keywords": [
    "demo",
    "typescript"
  ],
  "homepage": "https://github.com/retconned/kickbot-sdk",
  "bugs": {
    "url": "https://github.com/retconned/kickbot-sdk/issues"
  },
  "author": "retconned <retcnned@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retconned/kickbot-sdk.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx,jsx,js}\"",
    "check-format": "prettier --check \"src/**/*.{ts,tsx,jsx,js}\"",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@changesets/cli": "^2.27.8",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
